# m5squared Configuration Template
# Copy this file to .env and fill in your actual values
# The .env file is gitignored and will not be committed

# Left wheel Bluetooth MAC address (format: AA:BB:CC:DD:EE:FF)
M25_LEFT_MAC=

# Right wheel Bluetooth MAC address
M25_RIGHT_MAC=

# Left wheel AES encryption key (64 hex chars from QR code conversion)
# Use: python m25_qr_to_key.py "YourQRCodeString" to generate this
M25_LEFT_KEY=

# Right wheel AES encryption key
M25_RIGHT_KEY=

# Optional: Bluetooth RFCOMM channel (usually 6 for M25)
M25_RFCOMM_CHANNEL=6

# Optional: Connection timeout in seconds
M25_TIMEOUT=10
